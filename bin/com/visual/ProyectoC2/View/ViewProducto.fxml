<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visual.ProyectoC2.View.ViewProductoController">
   <children>
      <ImageView fx:id="Fondo" fitHeight="450.0" fitWidth="500.0" layoutX="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Resources/fondoProducto.jpg" />
         </image></ImageView>
      <ImageView fx:id="Logo" fitHeight="104.0" fitWidth="125.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="User" fitHeight="105.0" fitWidth="105.0" layoutX="398.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="Nuevo" layoutX="73.0" layoutY="321.0" mnemonicParsing="false" onMouseClicked="#botonNuevo" prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: #9C33A4;" text="Nuevo" textFill="WHITE" />
      <Button fx:id="Baja" layoutX="381.0" layoutY="321.0" mnemonicParsing="false" onMouseClicked="#botonBaja" prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: #9C33A4;" text="Baja" textFill="WHITE" />
      <Button fx:id="Reporte" layoutX="170.0" layoutY="321.0" mnemonicParsing="false" onMouseClicked="#botonReporte" prefHeight="25.0" prefWidth="61.0" style="-fx-background-color: #9C33A4;" text="Reporte" textFill="WHITE" />
      <TableView fx:id="Tabla" layoutX="53.0" layoutY="105.0" prefHeight="200.0" prefWidth="399.0">
        <columns>
          <TableColumn fx:id="Nombre" prefWidth="160.0" />
          <TableColumn fx:id="Precio" prefWidth="70.0" />
            <TableColumn fx:id="Cantidad" prefWidth="70.0" />
            <TableColumn fx:id="Codigo" prefWidth="100.0" />
        </columns>
      </TableView>
      <Button fx:id="Salir" layoutX="188.0" layoutY="416.0" mnemonicParsing="false" onMouseClicked="#botonSalir" prefHeight="25.0" prefWidth="125.0" style="-fx-background-color: #9C33A4;" text="Salir" textFill="WHITE" />
      <Button fx:id="Actualizar" layoutX="278.0" layoutY="321.0" mnemonicParsing="false" onMouseClicked="#botonActualizar" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #9C33A4;" text="Actualizar" textFill="WHITE" />
      <Button fx:id="Compras" layoutX="217.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#botonCompras" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #9C33A4;" text="Compras" textFill="WHITE" />
      <ImageView fx:id="Carrito" fitHeight="55.0" fitWidth="70.0" layoutX="363.0" layoutY="373.0" pickOnBounds="true" preserveRatio="true" />
      <Label alignment="CENTER" layoutX="170.0" layoutY="36.0" prefHeight="33.0" prefWidth="160.0" text="Productos" textFill="WHITE">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
